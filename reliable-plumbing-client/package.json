{
  "name": "reliable-plumbing",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:client-and-server-bundles:dev": "ng build --aot && ng build --aot --app 1 --output-hashing=false",
    "build:prerender:dev": "npm run build:client-and-server-bundles:dev && npm run webpack:server && npm run generate:prerender",
    "build:ssr:dev": "npm run build:client-and-server-bundles:dev && npm run webpack:server",
    "build:client-and-server-bundles:prod": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "build:prerender:prod": "npm run build:client-and-server-bundles:prod && npm run webpack:server && npm run generate:prerender",
    "build:ssr:prod": "npm run build:client-and-server-bundles:prod && npm run webpack:server",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.2",
    "@angular/animations": "^5.0.2",
