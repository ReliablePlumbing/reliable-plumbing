    "alertify.js": "^1.0.12",
    "angular2-text-mask": "^8.0.4",
    "core-js": "^2.4.1",
    "moment": "^2.19.1",
    "ng2-select": "^1.2.0",
    "primeng": "^5.0.2",
    "rxjs": "^5.4.3",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "uuid": "^3.1.0",
    "zone.js": "^0.8.14"
